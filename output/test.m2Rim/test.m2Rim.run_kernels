#!/bin/bash
#
echo "======================" > output/test.m2Rim/test.m2Rim.log
echo "Stripping MINEOS" >> output/test.m2Rim/test.m2Rim.log
#
../MINEOS/bin/mineos_strip <<! >> output/test.m2Rim/test.m2Rim.log
output/test.m2Rim/test.m2Rim.strip
output/test.m2Rim/test.m2Rim_0.eig_fix


!
#
echo "======================" > output/test.m2Rim/test.m2Rim.log
echo "Done stripping, now calculating tables" > output/test.m2Rim/test.m2Rim.log
#
../MINEOS/bin/mineos_table <<! >> output/test.m2Rim/test.m2Rim.log
output/test.m2Rim/test.m2Rim.table
40000
0 200.1
1 5500
output/test.m2Rim/test.m2Rim.q
output/test.m2Rim/test.m2Rim.strip

!
#
echo "======================" > output/test.m2Rim/test.m2Rim.log
echo "Creating branch file" > output/test.m2Rim/test.m2Rim.log
#
../MINEOS/bin/plot_wk <<! >> output/test.m2Rim/test.m2Rim.log
table output/test.m2Rim/test.m2Rim.table_hdr
search
1 0.0 200.1
99 0 0
branch

quit
!
#
echo "======================" > output/test.m2Rim/test.m2Rim.log
echo "Making frechet phV kernels binary" > output/test.m2Rim/test.m2Rim.log
#
if [ -f "output/test.m2Rim/test.m2Rim.cvfrechet" ]; then rm output/test.m2Rim/test.m2Rim.cvfrechet; fi
../MINEOS/bin/frechet_cv <<! >> output/test.m2Rim/test.m2Rim.log
./data/earth_models/qmod3
output/test.m2Rim/test.m2Rim.table_hdr.branch
output/test.m2Rim/test.m2Rim.cvfrechet
output/test.m2Rim/test.m2Rim_0.eig_fix
0


!
#
echo "======================" > output/test.m2Rim/test.m2Rim.log
